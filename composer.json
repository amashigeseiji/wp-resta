{
    "name": "wp/resta",
    "type": "wordpress-plugin",
    "autoload": {
        "psr-4": {
            "Wp\\Resta\\": "src/"
        }
    },
    "require": {
      "php": ">=8.2"
    },
    "require-dev": {
        "roots/wordpress": "^6.4",
        "szepeviktor/phpstan-wordpress": "^1.3",
        "phpstan/extension-installer": "^1.3",
        "phpstan/phpstan": "^1.10"
    },
    "config": {
        "allow-plugins": {
            "roots/wordpress-core-installer": true,
            "phpstan/extension-installer": true
        }
    }
}
